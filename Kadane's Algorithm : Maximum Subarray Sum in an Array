// Brute Force and somewhat optimised. Time = O(N^2)

#include <iostream> 
#include <climits>
#include <vector> 

using namespace std; 

int main() {
    int n; 
    cin >> n; 
    vector<int> numbers(n); 
    for (int i = 0; i < n; i += 1) cin >> numbers[i];

    int maxSum = INT_MIN;
    for (int i = 0; i < n; i += 1) {
        int flag = 0, maxEle = INT_MIN; 
        vector<int> SubArray; 
        
        for (int j = i; j < n; j += 1) {
            flag += numbers[j]; 
            SubArray.emplace_back(flag); 
        }
        for (int sA : SubArray) maxEle = max(maxEle, sA); 
        maxSum = max(maxEle, maxSum); 
    }
    cout << maxSum; 

    return 0; 
}
-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
// Optimal Approach Time = O(N)

#include <iostream> 
#include <vector> 

using namespace std; 

int isPositiveThere(vector<int> v) {
    for (int num : v) if (num >= 0) return 1; 
    return 0; 
}

int maxSubArray(const vector<int>& nums) {
    int flag = 0, maxSum = INT_MIN; 
    if (isPositiveThere(nums)) {
        for (int n : nums) {
            flag += n; 
            if (flag < 0) flag = 0; 
            maxSum = max(maxSum, flag);  
        }
    }
    else {
        for (int n : nums) maxSum = max(maxSum, n); 
    }
    return maxSum; 
}

int main() {
    int n; 
    cin >> n; 
    vector<int> numbers(n); 
    for (int i = 0; i < n; i += 1) cin >> numbers[i];

    cout << maxSubArray(numbers); 

    return 0; 
}

