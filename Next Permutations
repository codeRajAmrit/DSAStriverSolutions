// Better Approach Time = O(nlogn)

#include <iostream> 
#include <vector> 
using namespace std;

int main() {
    int n; 
    cin >> n; 
    vector<int> nums(n); 
    for (int i = 0; i < n; i += 1) cin >> nums[i];

    vector<int> digits = {nums[n-1]}; 
    for (int i = n-2; i >= 0; i--){
        digits.emplace_back(nums[i]); 
        if (nums[i] < nums[i+1]) {
            
            for (int j = 0; j < digits.size(); j += 1) {
                if (digits[j] > nums[i]) {
                    nums[i] = digits[j];
                    digits.erase(digits.begin() + j); 
                    break; 
                }
            }
            sort(digits.begin(), digits.end());
            for (int j = 0; j < digits.size(); j += 1) {
                nums[i+j+1] = digits[j]; 
            }
            break; 
        }
    }

    for (int k : nums) cout << k << " "; 

    return 0; 
}
